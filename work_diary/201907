const logArgs = (...args) => console.log(...args)
const favoriteFood = 'pizza'
logArgs(`I like ${favoriteFood}.`) // -> I like pizza.
logArgs`I like ${favoriteFood}.` // -> ["I like ", "."] "pizza"

